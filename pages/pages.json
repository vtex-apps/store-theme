{
  "pages": {
    "store": {
      "path": "/",
      "theme": "theme"
    },
    "store/home": {
      "path": "/"
    },
    "store/search": {
      "path": "/:term/s"
    },
    "store/product": {
      "path": "/:slug/p"
    }
  },
  "extensions": {
    "store": {
      "component": "StoreTemplate"
    },
    "store/home": {
      "component": "HomePage"
    },
    "store/home/carousel": {
      "component": "vtex.carousel/Carousel"
    },
    "store/home/shelf": {
      "component": "vtex.shelf/Shelf"
    },
    "store/home/shelf/shelfitem": {
      "component": "vtex.product-summary/ProductSummary"
    },
    "store/search": {
      "component": "SearchPage"
    },
    "store/product": {
      "component": "ProductPage"
    }
  }
}
