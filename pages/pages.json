{
  "extensions": {
    "store/theme": {
      "component": "Theme"
    },
    "store/__loading": {
      "component": "vtex.styleguide/Spinner"
    }
  },
  "templates": {
    "store": {
      "component": "vtex.render-runtime/LayoutContainer",
      "context": "vtex.store@1.x/StoreContextProvider",
      "props": {
        "elements": [
          "header",
          "__children__",
          "footer"
        ]
      },
      "extensions": {
        "header": {
          "component": "vtex.store-components/Header"
        },
        "header/minicart": {
          "component": "vtex.minicart/MiniCart"
        },
        "header/login": {
          "component": "vtex.login/Login"
        },
        "header/menu-link": {
          "component": "vtex.menu/Menu"
        },
        "header/category-menu": {
          "component": "vtex.category-menu/index"
        },
        "header/telemarketing": {
          "component": "vtex.telemarketing/index"
        },
        "footer": {
          "component": "vtex.store-components/Footer"
        }
      }
    },
    "product": {
      "component": "vtex.render-runtime/LayoutContainer",
      "context": "vtex.store@1.x/ProductContextProvider",
      "props": {
        "elements": [
          "breadcrumb",
          "details",
          "kit",
          "related"
        ]
      },
      "extensions": {
        "breadcrumb": {
          "component": "vtex.breadcrumb/Breadcrumb"
        },
        "details": {
          "component": "vtex.product-details/ProductDetails"
        },
        "kit": {
          "component": "vtex.product-kit/ProductKit"
        },
        "related": {
          "component": "vtex.shelf/RelatedProducts"
        }
      }
    },
    "search": {
      "component": "vtex.render-runtime/LayoutContainer",
      "context": "vtex.store@1.x/ProductSearchContextProvider",
      "props": {
        "elements": [
          "banner",
          "results"
        ]
      },
      "extensions": {
        "banner": {
          "component": "vtex.carousel/Carousel"
        },
        "results": {
          "component": "vtex.search-result/index"
        },
        "results/breadcrumb": {
          "component": "vtex.breadcrumb/Breadcrumb"
        }
      }
    },
    "login": {
      "component": "vtex.render-runtime/LayoutContainer",
      "props": {
        "elements": [
          "login"
        ]
      },
      "extensions": {
        "login": {
          "component": "vtex.login/LoginContent"
        }
      }
    },
    "home": {
      "component": "vtex.render-runtime/LayoutContainer",
      "props": {
        "elements": [
          "shelf",
          "carousel"
        ]
      },
      "extensions": {
        "carousel": {
          "component": "vtex.carousel/Carousel"
        },
        "shelf": {
          "component": "vtex.shelf/Shelf"
        }
      }
    },
    "account": {
      "component": "vtex.render-runtime/LayoutContainer",
      "props": {
        "elements": [
          "__children__"
        ]
      }
    },
    "orders": {
      "component": "vtex.my-orders-app/index"
    },
    "simple": {
      "component": "vtex.render-runtime/LayoutContainer",
      "props": {
        "elements": [
          "container"
        ]
      }
    },
    "department": {
      "component": "vtex.render-runtime/LayoutContainer",
      "context": "vtex.store@1.x/ProductSearchContextProvider",
      "props": {
        "elements": [
          "header",
          "results"
        ]
      },
      "extensions": {
        "header": {
          "component": "vtex.render-runtime/ExtensionContainer"
        },
        "header/carousel": {
          "component": "vtex.carousel/Carousel"
        },
        "header/categories": {
          "component": "vtex.store-components/CategoriesHighlights"
        },
        "results": {
          "component": "vtex.search-result/index"
        }
      }
    }
  },
  "pages": {
    "store": [
      {
        "name": "Default",
        "template": "store"
      }
    ],
    "store/home": [
      {
        "name": "Default",
        "template": "home"
      }
    ],
    "store/account": [
      {
        "name": "Default",
        "template": "account"
      }
    ],
    "store/account/orders": [
      {
        "name": "Default",
        "template": "orders"
      }
    ],
    "store/compare": [
      {
        "name": "Default",
        "template": "simple"
      }
    ],
    "store/login": [
      {
        "name": "Default",
        "template": "login"
      }
    ],
    "store/brand": [
      {
        "name": "Default",
        "template": "search"
      }
    ],
    "store/department": [
      {
        "name": "Default",
        "template": "department"
      }
    ],
    "store/category": [
      {
        "name": "Default",
        "template": "search"
      }
    ],
    "store/subcategory": [
      {
        "name": "Default",
        "template": "search"
      }
    ],
    "store/search": [
      {
        "name": "Default",
        "template": "search"
      }
    ],
    "store/product": [
      {
        "name": "Default",
        "template": "product"
      }
    ]
  }
}
