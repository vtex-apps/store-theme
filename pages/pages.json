{
  "pages": {
    "store": {
      "path": "/",
      "theme": "theme"
    },
    "store/home": {
      "path": "/"
    },
    "store/search": {
      "path": "/:term/s"
    },
    "store/product": {
      "path": "/:slug/p"
    }
  },
  "extensions": {
    "store": {
      "component": "StoreTemplate"
    },
    "store/footer": {
      "component": "vtex.storecomponents/Footer"
    },
    "store/minicart": {
      "component": "vtex.minicart/MiniCart"
    },
    "store/menu-link": {
      "component": "vtex.menu/Menu"
    },
    "store/category-menu": {
      "component": "vtex.category-menu/CategoryMenu"
    },
    "store/home": {
      "component": "HomePage"
    },
    "store/home/gallery": {
      "component": "vtex.carousel/Carousel"
    },
    "store/home/shelf": {
      "component": "vtex.shelf/Shelf"
    },
    "store/home/custom": {
      "component": "vtex.shelf/Shelf"
    },
    "store/search": {
      "component": "SearchPage"
    },
    "store/search/gallery": {
      "component": "vtex.gallery/Gallery"
    },
    "store/product": {
      "component": "ProductPage"
    },
    "store/product/product-details": {
      "component": "vtex.product-details/ProductDetails"
    }
  }
}