{
  "pages": {
    "store": {
      "path": "/",
      "theme": "theme"
    },
    "store/home": {
      "path": "/"
    },
    "store/cart": {
      "path": "/cart/"
    },
    "store/search": {
      "path": "/s/:term"
    },
    "store/product": {
      "path": "/p/:id"
    }
  },
  "extensions": {
    "store": {
      "component": "StoreTemplate"
    },
    "store/home": {
      "component": "HomePage"
    },
    "store/home/banner": {
      "component": "Banner",
      "props": {
        "image": "http://clipart-library.com/newimages/clip-art-banner-7.png"
      }
    },
    "store/home/carousel": {
      "component": "Carousel",
      "props": {
        "banner1": "http://clipart-library.com/newimages/clip-art-banner-7.png",
        "banner2": "http://clipart-library.com/newimages/clip-art-banner-7.png"
      }
    },
    "store/home/shelf": {
      "component": "Shelf",
      "props": {
        "from": 0,
        "to": 8,
        "orderBy": "OrderByTopSaleDESC"
      }
    },
    "store/cart": {
      "component": "CartPage"
    },
    "store/search": {
      "component": "SearchPage"
    },
    "store/product": {
      "component": "ProductPage"
    }
  }
}
