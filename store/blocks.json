{
  "store.home": {
    "blocks": ["carousel#home", "shelf#home"]
  },
  "store.product": {
    "blocks": [
      "product-details#default",
      "product-kit",
      "shelf.relatedProducts"
    ]
  },
  "store.search": {
    "blocks": ["search-result"]
  },
  "store.search#brand": {
    "blocks": ["search-result"]
  },
  "store.search#department": {
    "blocks": ["search-result#department"]
  },
  "store.search#category": {
    "blocks": ["search-result"]
  },
  "store.search#subcategory": {
    "blocks": ["search-result"]
  },
  "store.account": {
    "blocks": ["my-account"],
    "parent": {
      "challenge": "challenge.profile"
    }
  },
  "store.login": {
    "blocks": ["login-content#default"]
  },
  "store.orderplaced": {
    "blocks": [
      "order-placed"
    ]
  },
  "header": {
    "blocks": [
      "logo"
    ]
  },
  "header.full": {
    "blocks": [
      "login",
      "minicart",
      "logo",
      "search-bar",
      "menu-link",
      "telemarketing",
      "category-menu"
    ]
  },
  "menu-link": {
    "props": {
      "numberOfItems": 6,
      "item1": {
        "title": "Atendimento",
        "typeOfRoute": "internal",
        "page": "store/home",
        "position": "right"
      },
      "item2": {
        "title": "Programa de Afiliados",
        "typeOfRoute": "internal",
        "position": "middle"
      },
      "item3": {
        "title": "Anuncie",
        "typeOfRoute": "internal",
        "position": "middle"
      },
      "item4": {
        "title": "Televendas 4003-2018",
        "typeOfRoute": "internal",
        "position": "left"
      },
      "item5": {
        "title": "Sobre",
        "typeOfRoute": "internal",
        "position": "right"
      }
    }
  },
